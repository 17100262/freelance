<p id="notice"><%= notice %></p>

<h1>Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Title</th>
      <th>Description</th>
      <th>Deliverables</th>
      <th>Duration</th>
      <th>Language</th>
      <th>Amount</th>
      <th>Online</th>
      <th>Address</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.status %></td>
        <td><%= job.title %></td>
        <td><%= job.description %></td>
        <td><%= job.deliverables %></td>
        <td><%= job.duration %></td>
        <td><%= job.language %></td>
        <td><%= job.amount %></td>
        <td><%= job.online %></td>
        <td><%= job.address %></td>
        <td><%= job.user %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
