<div class="container-fluid p-0 pt-4" style="background:#f5f5f5; min-height:70vh;">

  <div class="row" style="margin-right:0px;">
    <div class="col-md-2 hidden-sm hidden-xs"></div>
      <div class="col-md-8 col-sm-12 col-xs-12">
        <ul class="notificator">
          <div id="notificationContainer">
            <center><h2 id="notificationTitle">Notifications</h2></center>
            <div id="notificationsBody" class="notifications">
              <ul id="notificationList" style="padding-left:0 !important;">
                <%#= render notifications %>
                <%= render partial: "notifications/notification", collection: notifications %>

              </ul>
            </div>
            <div id="notificationFooter"></div>
          </div>
        </ul> 
      </div>
    <div class="col-md-2 hidden-sm hidden-xs"></div>  
  </div>
</div>